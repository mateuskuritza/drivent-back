{
  "name": "typescript-back-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test npm run eslint:fix && npx jest -i --verbose",
    "build": "NODE_ENV=production rm -rf ./dist && npm run eslint:fix && npx ttsc -P ./tsconfig.json --outDir dist && babel dist -d dist",
    "start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/server.js",
    "dev": "NODE_ENV=development npx nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"npm run eslint:fix && ts-node -r tsconfig-paths/register --files src/server.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run build && npm run typeorm migration:generate --",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "eslint": "npx eslint src",
    "eslint:fix": "npx eslint --fix src",
    "seed": "ts-node -r tsconfig-paths/register ./src/seeders",
    "seed:production": "cp src/seeders/*.seed.yml dist/seeders && NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/seeders"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npx pretty-quick --staged",
      "npx eslint --fix src --ext .ts",
      "npx eslint --fix tests --ext .ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/email-validator": "^1.0.6",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^26.0.24",
    "@types/joi": "^17.2.3",
    "@types/js-yaml": "^4.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.10",
    "@types/node": "^16.3.3",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.1",
    "@types/readline-sync": "^1.4.4",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "jest-mock-extended": "^2.0.2-beta2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "aws-sdk": "^2.988.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.5.0",
    "joi": "^17.4.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.6.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "multer-s3": "^2.9.0",
    "pg": "^8.6.0",
    "prettier": "^2.4.1",
    "readline-sync": "^1.4.10",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.34",
    "typescript-transform-paths": "^3.3.1",
    "uuid": "^8.3.2"
  }
}
